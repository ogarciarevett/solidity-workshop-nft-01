# Sei Testnet Configuration
# Copy this file to .env and fill in your values

# RPC URL for Sei Testnet
SEI_TESTNET_RPC_URL=https://evm-rpc-testnet.sei-apis.com

# Your wallet private key (without 0x prefix)
# NEVER commit your actual private key to version control!
PRIVATE_KEY=your_private_key_here

# Seitrace API key for contract verification
# Get one from https://seitrace.com/
SEI_TESTNET_ETHERSCAN_KEY=your_seitrace_api_key_here

# Optional: Deployed contract addresses (fill after deployment)
SEIMONS_ADDRESS=
SEIMONS_RANDOM_ADDRESS=
SEIMONS_ASSEMBLY_ADDRESS=

# Optional: Custom gas settings
GAS_PRICE=10000000000
GAS_LIMIT=5000000
